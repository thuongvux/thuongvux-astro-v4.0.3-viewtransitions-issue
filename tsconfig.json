{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "@backend/*": ["src/backend/*"],
      "@components/*": ["src/components/*"],
      "@configs/*": ["src/configs/*"],
      "@helpers/*": ["src/helpers/*"],
      "@layouts/*": ["src/layouts/*"],
      "@public/*": ["public/*"]
    }
  },
  "extends": "astro/tsconfigs/strict"
}
